  "bugs": {
    "url": "https://github.com/rendall/rendall.github.io/issues"
  },
  "homepage": "https://github.com/rendall/rendall.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.6.0",
    "@11ty/eleventy-plugin-rss": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.0",
    "@types/fontfaceobserver": "^0.0.6",
    "luxon": "^1.8.2",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "prettier": "1.16.1",
    "sass": "^1.14.3",
    "string": "^3.3.3",
    "stylelint": "^9.10.1",
    "typescript": "^3.5.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
